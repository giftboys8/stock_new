# 数据库配置
DATABASE_URL=sqlite:///./stock_analysis.db

# API配置
API_HOST=0.0.0.0
API_PORT=8000
API_PREFIX=/api

# 数据更新配置
UPDATE_INTERVAL_MINUTES=60
ENABLE_SCHEDULER=true

# akshare配置
